<%= form_for(manga) do |f| %>

  <div class="field">
    <%= f.label :titre %>
    <%= f.text_field :titre %>
  </div>

  <div class="field">
    <%= f.label :thumbnail %>
    <%= f.file_field :thumbnail %>
  </div>

  <div class="field">
    <label for="images[]">Multi Upload tavu</label><br />
    <%= file_field_tag 'images[]', type: :file, multiple: true %>
  </div>



  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
